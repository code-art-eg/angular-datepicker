{
  "name": "@code-art/angular-datepicker",
  "version": "2.0.0",
  "description": "A datepicker for angular 6",
  "main": "index.js",
  "scripts": {
    "test": "cd test && mocha --require mock-local-storage --require ts-node/register ./**/*.spec.ts",
    "start": "cd playground && webpack --config webpack.config.vendor.js && webpack-dev-server --open",
    "build": "ng-packagr -p package.json",
    "lint": "tslint --project . && tslint --project ./test && tslint --project ./playground",
    "icons": "cd ./tools && ts-node  build-icons.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sherif-elmetainy/angular-datepicker.git"
  },
  "keywords": [
    "angular",
    "datepicker"
  ],
  "author": "Sherif Elmetainy (Code Art)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sherif-elmetainy/angular-datepicker/issues"
  },
  "homepage": "https://github.com/sherif-elmetainy/angular-datepicker#readme",
  "dependencies": {
    "@code-art/angular-globalize": "^2.0.2",
    "cldr-data": "^33.0.0"
  },
  "peerDependencies": {
    "@angular/common": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@code-art/angular-globalize": "^2.0.1",
    "cldr-data": ">=32.0.1",
    "globalize": "^1.3.0",
    "rxjs": "^6.1.0"
  },
  "devDependencies": {
    "@angular/cli": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@types/chai": "^4.1.2",
    "@types/chai-datetime": "0.0.31",
    "@types/globalize": "^0.0.34",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.5",
    "@types/webpack": "^3.8.5",
    "@types/webpack-env": "^1.13.5",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.4.1",
    "bootstrap": "^3.3.6",
    "chai": "^4.1.2",
    "chai-datetime": "^1.5.0",
    "change-case": "^3.0.2",
    "css-loader": "^0.28.9",
    "es6-promise": "^4.2.4",
    "es6-shim": "^0.35.3",
    "event-source-polyfill": "0.0.12",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "globalize": "^1.3.0",
    "html-loader": "^0.5.5",
    "iana-tz-data": "^2017.1.0",
    "imports-loader": "^0.7.1",
    "jquery": "^3.3.1",
    "json-loader": "^0.5.7",
    "less": "^2.3.1",
    "less-loader": "^4.0.5",
    "mocha": "^5.0.0",
    "mock-local-storage": "^1.0.5",
    "ng-packagr": "^2.4.5",
    "reflect-metadata": "^0.1.12",
    "to-string-loader": "^1.1.5",
    "ts-node": "^6.0.2",
    "tsickle": "0.32.1",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "typescript": "^2.9.0",
    "url-loader": "^1.1.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-hot-middleware": "^2.21.0",
    "zone.js": "^0.8.26"
  },
  "ngPackage": {
    "$schema": "./node_modules/ng-packagr/ng-package.schema.json",
    "workingDirectory": "./build",
    "lib": {
      "entryFile": "./src/module.ts",
      "umdModuleIds": {
        "@code-art/angular-globalize": "code-art.angular-globalize"
      }
    },
    "dest": "./dist/angular-datepicker"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
